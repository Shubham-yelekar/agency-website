{
  "format": "custom",
  "id": "project",
  "label": "project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "name": {
        "type": "Text",
        "config": {
          "label": "name",
          "placeholder": "project name"
        }
      },
      "cover_image": {
        "type": "Image",
        "config": {
          "label": "cover image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "date",
          "placeholder": "date"
        }
      },
      "is_featured": {
        "type": "Boolean",
        "config": {
          "label": "is_featured",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "description",
          "placeholder": "description",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      },
      "client": {
        "type": "Link",
        "config": {
          "label": "client",
          "select": "document",
          "repeat": false,
          "customtypes": ["client"]
        }
      },
      "industries": {
        "type": "Link",
        "config": {
          "label": "industries",
          "select": "document",
          "repeat": false,
          "customtypes": ["industry"]
        }
      },
      "project_services": {
        "type": "Group",
        "config": {
          "label": "Project Services",
          "repeat": false,
          "fields": {
            "service": {
              "type": "Link",
              "config": {
                "label": "Service",
                "select": "document",
                "repeat": false,
                "customtypes": ["service"]
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "project_images": {
              "type": "SharedSlice"
            },
            "project_video": {
              "type": "SharedSlice"
            },
            "project_embed": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
